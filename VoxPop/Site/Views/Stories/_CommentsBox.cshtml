@model IEnumerable<Site.Storage.Models.CommentEntity>
@Styles.Render("~/Content/css")
@{
    Layout = "";
}

@section MetaTags
{
    @Html.Partial("_DefaultMetaTags")
}

<h2>Voter Comments</h2>

@if (@Model != null)
{
    for (var i = 0; i < Model.Count(); i++)
    {
        if (Model.ElementAt(i).PollOptionKey.Count() <= 2)
        {
            foreach (var voteComm in Model)
            {
                @voteComm.Comment;
            }
        }
    }

}

